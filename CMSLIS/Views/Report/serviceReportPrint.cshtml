                                            <span style="display:none">
                                                @(STT = STT + 1)
                                                @(total = total + data.Total_Price);
                                                @(Count = Count + data.Count_Service);
                                                @(totalAll = totalAll + data.Total_Price);
                                                @(CountAll = CountAll + data.Count_Service);
                                            </span>
                                        </tr>
                                    }
                                }


                                <tr>

                                    <td colspan="2" style="font-weight:bold;">Tổng</td>
