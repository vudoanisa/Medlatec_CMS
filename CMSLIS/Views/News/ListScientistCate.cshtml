@using CMS_Core.Entity;
@model  CMSLIS.Models.SearchNewsViewModel
@{
    ViewBag.Title = "Danh sách chuyên khoa";
    var cms_Scientist_Cates = (List<cms_Scientist_Cate>)ViewBag.cms_Scientist_Cates;
}


<script src="/Content/AdminLTE/js/Customer/ListScientistCate.js"></script>
<!-- Head Libs -->
<script src="/Content/AdminLTE/vendor/modernizr/modernizr.js"></script>


 


    <div class="box box-info" style="margin-top: 5px;">
        <section class="panel">
            <!-- Begin Content -->
            <div class="panel-body">
                <div class="row" style="margin-bottom: 5px;margin-left: -5px;">
                    <button type="button" id="ListScientistCateAdd" value="ListScientistCateAdd" class="btn btn-info mr-1 mb-2" onclick="return ConfirmAdd();"><i class="fa fa-plus-square-o"></i> Thêm mới (Ctrl + F1) </button>
                    <button type="button" id="ListScientistCateDelete" value="ListScientistCateDelete" class="btn btn-danger mr-1 mb-2" style="width:100px;" onclick="return ConfirmDelete();"><i class="fa fa-remove"></i> Xóa </button>
                    <button type="button" id="ListScientistCatePublic" value="ListScientistCatePublic" class="btn btn-info mr-1 mb-2" style="width:100px;" onclick="return ConfirmPublic();"><i class="fa fa-check-square-o"></i> Duyệt </button>
                </div>

                <!-- Sorting -->
                <table id="sorting-table" class="table table-bordered table-striped dataTable" role="grid">
                    <thead>
                        <tr>
                            <th><input type="checkbox" id="checkBoxAll" class="custom-checkbox" /></th>

                            <th>Tên chuyên khoa</th>
                            <th>Mô tả chuyên khoa</th>
                            <th>Ngày tạo</th>
                            <th><span style="width:100px;">Trạng thái</span></th>

                            <th id="edit">Edit</th>
                        </tr>
                    </thead>
                    <tbody>
                        @if (cms_Scientist_Cates != null)
                        {
                            foreach (var data in cms_Scientist_Cates)
                            {
                                <tr>
                                    <td><input type="checkbox" name="ID" value="@data.ID" class="checkBox custom-checkbox chkCheckBoxId" /></td>
 

                                    <td>
                                        <span class="text-primary">
                                            @Server.HtmlEncode(data.ScientistTitle)
                                        </span>
                                    </td>

                                    <td>@Server.HtmlEncode(data.ScientistDesc)</td>
                                    <td>@data.dateCreate.ToString("dd/MM/yyyy")</td>

                                    <td>@Html.Raw(CMSLIS.Common.Common.getTypeStatus(data.Status))</td>
                                    <td class="td-actions">
                                        <a href="/News/ListScientistCateAdd?Id=@data.ID"><i class="fa fa-edit edit"></i></a>
                                    </td>
                                </tr>

                            }
                        }
                    </tbody>
                </table>

                <!-- End Sorting -->

            </div>
            <!-- End Content -->

        </section>
    </div>
 

<script src="/content/AdminLTE/vendor/nanoscroller/nanoscroller.js"></script>

<script src="/content/AdminLTE/vendor/magnific-popup/magnific-popup.js"></script>
<script src="/content/AdminLTE/vendor/jquery-placeholder/jquery.placeholder.js"></script>


<!-- Specific Page Vendor -->
<script src="/Content/AdminLTE/vendor/select2/select2.js"></script>
<script src="/Content/AdminLTE/vendor/jquery-datatables/media/js/jquery.dataTables.js"></script>
<script src="/Content/AdminLTE/vendor/jquery-datatables/extras/TableTools/js/dataTables.tableTools.min.js"></script>
<script src="/Content/AdminLTE/vendor/jquery-datatables-bs3/assets/js/datatables.js"></script>

<!-- Theme Base, Components and Settings -->
<script src="/Content/AdminLTE/javascripts/theme.js"></script>

<!-- Theme Custom -->
<script src="/Content/AdminLTE/javascripts/theme.custom.js"></script>

<!-- Theme Initialization Files -->
<script src="/Content/AdminLTE/javascripts/theme.init.js"></script>
<script src="/Content/AdminLTE/js/adminlte.js"></script>
<script src="/Content/AdminLTE/js/init.js"></script>

