@using CMS_Core.Entity;
@model  CMSLIS.Models.SearchMenuViewModel
@{
    ViewBag.Title = "Danh sách mẫu nhập kết quả";
    var TEMPLATERESULTEXAMINATIONS = (List<CMS_TEMPLATERESULTEXAMINATION>)ViewBag.TEMPLATERESULTEXAMINATIONS;
}

<script src="/Content/AdminLTE/js/Customer/ListTemplateResultExamination.js"></script>
<!-- Head Libs -->
<script src="/Content/AdminLTE/vendor/modernizr/modernizr.js"></script>


<!-- Begin Content -->
<div class="box box-info">

    @using (Html.BeginForm("ListTemplateResultExamination", "CatalogSystem", new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post, new { role = "form", @class = "form-horizontal" }))
    {
        <div class="box-body">
            <div class="col-sm-3">
                <div class="form-group align-content-center">
                    <label class="control-label col-xs-4" style="font-weight:bold; ">Trạng thái: </label>
                    <div class="col-xs-8">
                        @Html.DropDownListFor(m => m.Status, this.ViewBag.GetTypeStatus as List<SelectListItem>, new { @class = "form-control select2", style = "width: 90%;", onchange = "SelectedValue(this)" })
                    </div>
                </div>
            </div>

            <div class="col-sm-2">
                <div class="form-group">
                    <button type="submit" id="cmdSearch" name="submit" value="SearchListTemplateResultExamination" class="btn btn-primary"><i class="fa fa-search"></i> Tìm kiếm</button>
                </div>
            </div>
        </div>
    }

    <div class="box box-info" style="margin-top: 5px;">
        <!-- Sorting -->
        <section class="panel">


            <!-- Begin Content -->
            <div class="panel-body">

                <div class="row" style="margin-bottom: 5px;margin-left: -5px;">
                    <a id="ListTemplateResultExaminationNew" href="/CatalogSystem/ListTemplateResultExaminationAdd" class="btn btn-info mr-1 mb-2"><i class="fa fa-plus-square-o"></i> Thêm mới (Ctrl + F1) </a>                    
                    <button type="button" id="ListTemplateResultExaminationDelete" value="ListTemplateResultExaminationDelete" class="btn btn-danger mr-1 mb-2" style="width:100px;" onclick="return ConfirmDelete();"><i class="fa fa-remove"></i> Xóa </button>
                    <button type="button" id="ListTemplateResultExaminationPublic" value="ListTemplateResultExaminationPublic" class="btn btn-info mr-1 mb-2" style="width:100px;" onclick="return ConfirmPublic();"><i class="fa fa-check-square-o"></i> Duyệt </button>
                </div>

                <table id="sorting-table" class="table table-bordered table-striped dataTable" role="grid">
                    <thead>
                        <tr>
                            <th><input type="checkbox" id="checkBoxAll" class="custom-checkbox" /></th>
                            <th>Tên mẫu</th>
                            <th>Nhóm dịch vụ</th>
                            <th>Dịch vụ</th>
                            <th><span style="width:100px;">Trạng thái</span></th>
                            <th>Edit</th>
                        </tr>
                    </thead>
                    <tbody>
                        @if (TEMPLATERESULTEXAMINATIONS != null)
                        {
                            foreach (var data in TEMPLATERESULTEXAMINATIONS)
                            {
                                <tr>
                                    <td><input type="checkbox" name="ID" value="@data.ID" class="checkBox custom-checkbox chkCheckBoxId" /></td>
                                    <td>@data.NAME</td>
                                    <td>@data.GROUP_SERVICE_NAME</td>
                                    <td>@data.SERVICE_NAME</td>

                                    <td style="width: 120px; ">@Html.Raw(CMSLIS.Common.Common.getTypeStatus(data.STATUS))</td>

                                    <td class="td-actions">
                                        <a href="/CatalogSystem/ListTemplateResultExaminationAdd?ID=@data.ID"><i class="fa fa-edit edit"></i></a>
                                    </td>
                                </tr>
                            }
                        }

                    </tbody>
                </table>

            </div>
        </section>
        <!-- End Sorting -->
    </div>

</div>
<!-- End Content -->

<script src="/content/AdminLTE/vendor/nanoscroller/nanoscroller.js"></script>

<script src="/content/AdminLTE/vendor/magnific-popup/magnific-popup.js"></script>
<script src="/content/AdminLTE/vendor/jquery-placeholder/jquery.placeholder.js"></script>


<!-- Specific Page Vendor -->
<script src="/Content/AdminLTE/vendor/select2/select2.js"></script>
<script src="/Content/AdminLTE/vendor/jquery-datatables/media/js/jquery.dataTables.js"></script>
<script src="/Content/AdminLTE/vendor/jquery-datatables/extras/TableTools/js/dataTables.tableTools.min.js"></script>
<script src="/Content/AdminLTE/vendor/jquery-datatables-bs3/assets/js/datatables.js"></script>

<!-- Theme Base, Components and Settings -->
<script src="/Content/AdminLTE/javascripts/theme.js"></script>

<!-- Theme Custom -->
<script src="/Content/AdminLTE/javascripts/theme.custom.js"></script>

<!-- Theme Initialization Files -->
<script src="/Content/AdminLTE/javascripts/theme.init.js"></script>
